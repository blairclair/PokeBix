layout-macro-def(poke-details-macro) {
  params {
    param (characteristic) {
      type (characteristic)
      min (Required)
      max (One)
    }
  }

  content {
    section {  // this macro returns a "section"
      content {
        paragraph {
          value ("#{value(characteristic.)}")
          style (Detail_M)
        }
      }
    }
  }
}
